# Frontend environment configuration
# Copy to .env and adjust for your environment

# Base URL used by fetch calls inside the SPA (defaults to /api)
VITE_API_BASE_URL=/api

# Vite dev server overrides
VITE_DEV_SERVER_PORT=5173
VITE_API_PROXY_TARGET=http://localhost:5050

# Toggle Vercel Speed Insights (set to false to disable telemetry in prod)
VITE_ENABLE_SPEED_INSIGHTS=true

# Toggle Vercel Analytics (set to false to disable web analytics tracking)
VITE_ENABLE_VERCEL_ANALYTICS=true

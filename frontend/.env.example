# Frontend environment configuration
# Copy to .env and adjust for your environment

# Base URL used by fetch calls inside the SPA (defaults to /api)
VITE_API_BASE_URL=/api

# Vite dev server overrides
VITE_DEV_SERVER_PORT=5173
VITE_API_PROXY_TARGET=http://localhost:5050

# Toggle Vercel Speed Insights (set to false to disable telemetry in prod)
VITE_ENABLE_SPEED_INSIGHTS=true

# Toggle Vercel Analytics (set to false to disable web analytics tracking)
VITE_ENABLE_VERCEL_ANALYTICS=true

# Allowed hosts for Vite preview server (comma-separated)
VITE_PREVIEW_ALLOWED_HOSTS=

# Telegram IDs of administrators (comma-separated)
# Replace with actual Telegram numeric IDs
VITE_ADMIN_TELEGRAM_IDS=123456789,987654321

# Telegram Bot configuration for RequireTelegram landing page
# Full URL to your bot (overrides username construction if set)
VITE_TELEGRAM_BOT_URL=

# Bot username (without @) - used to build t.me/<username> link if VITE_TELEGRAM_BOT_URL is empty
VITE_TELEGRAM_BOT_USERNAME=BlackjackCasinoBot

# Optional start parameter appended as ?startapp=<param> for WebApp deep linking
VITE_TELEGRAM_START_PARAM=